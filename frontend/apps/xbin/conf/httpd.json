{
    "extensions": [
        "proxy"
    ],
    "proxies": [
        {
            "^http(.*)://([^:/]+)[:]?.*?/apps/xbin/proxiedapis/(.*)$": "http{{{$1}}}://{{{$2}}}:9090/apps/xbin/proxiedapis/{{{$3}}}"
        }
    ],
    "diskCache": {
        "refresh": 1000,
        "dontCache": [
            ".*\\.pdf"
        ],
        "maxSizeInMB": "1024",
        "ignoreAfterMaxHits": 10
    },
    "httpdHeaders": {
        "Cache-Control": "max-age=604800, must-revalidate, private"
    }
}